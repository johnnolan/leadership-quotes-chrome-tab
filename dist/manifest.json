{
  "name": "New Tab Inspirational Leadership Quotes",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A new default tab for quotes from leaders and coaches. Initial version just has Simon Sinek.",
  "homepage_url": "https://www.nolanscafe.co.uk",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "bg/background.html",
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "override/override.html"
  }
}